{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${maven_group}",
    "id": "orbit",
    "version": "${version}",
    "metadata": {
      "name": "Orbit",
      "description": "An exploration and story-based mod for 1.18.2+.",
      "contributors": {
        "peaceheis": "Owner",
        "Icee50": "Owner"
      },
      "icon": "assets/orbit/icon.jpeg"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.github.the.bennett.group.Orbit"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${loader_version}"
      },
      {
        "id": "quilted_fabric_api",
        "versions": "${quilted_fabric_api_version}"
      },
      {
        "id": "minecraft",
        "versions": ">=${minecraft_version}"
      }
    ]
  },
  "mixin": [
    "orbit.mixins.json"
  ],
  "minecraft": {
    "environment": "*"
  }
}